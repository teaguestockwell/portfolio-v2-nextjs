{
  "name": "nextjs-static-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pretty": "npx prettier --write .",
    "lint": "next lint",
    "dev": "next dev",
    "build": "npx contentlayer2 build && next build --webpack",
    "start": "next start -p 3000",
    "prepare": "husky install",
    "analyze": "ANALYZE=true npm run build"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "resolutions": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  },
  "dependencies": {
    "@csstools/convert-colors": "^2.0.0",
    "@ducanh2912/next-pwa": "^10.2.9",
    "contentlayer2": "^0.5.8",
    "next": "^16.1.1",
    "next-compose-plugins": "^2.2.1",
    "next-contentlayer2": "^0.5.8",
    "next-seo": "^5.4.0",
    "next-themes": "^0.1.1",
    "preact": "^10.7.1",
    "react": "^19.0.0",
    "react-card-flip": "^1.2.3",
    "react-dancing-lines": "^1.1.4",
    "react-dom": "^19.0.0",
    "react-icon-cloud": "^4.0.1",
    "react-lazyload": "^3.2.0",
    "react-scroll": "1.9.3",
    "react-use": "^17.3.2",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-code-titles": "^1.2.1",
    "rehype-prism-plus": "^2.0.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "simple-icons": "^7.5.0",
    "ua-parser-js": "^1.0.2",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.5",
    "@types/lodash": "^4.14.182",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/react-lazyload": "^3.2.0",
    "@types/react-scroll": "1.8.10",
    "@types/simple-icons": "^5.21.1",
    "@types/ua-parser-js": "^0.7.36",
    "eslint": "^8.13.0",
    "eslint-config-next": "^16.1.1",
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "typescript": "^5.9.3"
